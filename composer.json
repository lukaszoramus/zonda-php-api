{
    "name": "lukaszoramus/zonda-php-api",
    "type": "library",
    "license": "MIT",
    "description": "Zonda PHP Api Client",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "ZondaPhpApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZondaPhpApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "≈Åukasz Oramus",
            "email": "lukaszoramus@gmail.com",
            "homepage": "https://lukaszoramus.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "php-http/discovery": "^1.14",
        "php-http/message": "^1.13",
        "php-http/curl-client": "^2.2",
        "php-http/client-common": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "vimeo/psalm": "^5.6"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
